@article{stephens_assessing_2010,
	series = {Energy {Security} - {Concepts} and {Indicators} with regular papers},
	title = {Assessing innovation in emerging energy technologies: {Socio}-technical dynamics of carbon capture and storage ({CCS}) and enhanced geothermal systems ({EGS}) in the {USA}},
	volume = {38},
	issn = {0301-4215},
	shorttitle = {Assessing innovation in emerging energy technologies},
	url = {https://www.sciencedirect.com/science/article/pii/S0301421509009392},
	doi = {10.1016/j.enpol.2009.12.003},
	abstract = {This study applies a socio-technical systems perspective to explore innovation dynamics of two emerging energy technologies with potential to reduce greenhouse gas emissions from electrical power generation in the United States: carbon capture and storage (CCS) and enhanced geothermal systems (EGS). The goal of the study is to inform sustainability science theory and energy policy deliberations by examining how social and political dynamics are shaping the struggle for resources by these two emerging, not-yet-widely commercializable socio-technical systems. This characterization of socio-technical dynamics of CCS and EGS innovation includes examining the perceived technical, environmental, and financial risks and benefits of each system, as well as the discourses and actor networks through which the competition for resources – particularly public resources – is being waged. CCS and EGS were selected for the study because they vary considerably with respect to their social, technical, and environmental implications and risks, are unproven at scale and uncertain with respect to cost, feasibility, and life-cycle environmental impacts. By assessing the two technologies in parallel, the study highlights important social and political dimensions of energy technology innovation in order to inform theory and suggest new approaches to policy analysis.},
	language = {en},
	number = {4},
	urldate = {2021-06-02},
	journal = {Energy Policy},
	author = {Stephens, Jennie C. and Jiusto, Scott},
	month = apr,
	year = {2010},
	keywords = {Carbon capture and storage (CCS), Geothermal power, Socio-technical systems},
	pages = {2020--2031},
}

@book{holl_what_2015,
	title = {What did we learn about {EGS} in the {Cooper} {Basin}?},
	abstract = {This report tries to summarise the experiences Geodynamics Limited had during over 12 years of operations in the Cooper Basin geothermal project. The document is not claiming to be complete in terms of listing all technical details but gives an extensive overview. It is listing all relevant internal and external technical reports as references for additional reading if someone has to find supplementary information.
The Habanero reservoir will be described in detail after summarizing exploration, drilling and stimulation activities. This includes petrological description of the host rock (Big Lake Suite Granite) as well as reservoir environmental conditions and reservoir fluid chemistry.
Technologies will be explained which were used to identify faults/fractures within the granite. The Habanero Fault model will be presented and the geomechanical conditions discussed.
A major step forward for the recent understanding of the reservoir was the completion of the reservoir modelling after the Habanero 4 stimulation and testing operations. The results delivered crucial input for Habanero Field Development Plan.
The conclusions presented at the end of this report are the results of a learning process lasting more than 12 years. There is no doubt that the interpretation of the data available to date and the modelling work performed so far will change with every new bit of information which might be added through future exploration and drilling.},
	author = {Holl, Heinz-Gerd},
	month = mar,
	year = {2015},
	doi = {10.13140/RG.2.2.33547.49443},
}

